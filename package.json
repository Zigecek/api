      "require-await": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zigecek/api.git"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.4.0"
  }
}

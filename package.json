{
  "version": "0.0.4",
  "main": "api.js",
  "scripts": {
    "start": "pm2 start ./api.js --time -- trace-warnings",
    "start2": "node ./api.js",
    "i": "npm i express@latest dotenv@latest mongoose@latest body-parser@latest",
    "uni": "npm uninstall dotenv express mongoose body-parser"
  },
  "author": "Jan Kozohorsk√Ω",

{
  "version": "0.0.8",
  "main": "api.js",
  "scripts": {
    "start": "pm2 start ./api.js --time -- trace-warnings",
    "start2": "node ./api.js",
    "i": "npm i express@latest dotenv@latest mongoose@latest body-parser@latest cors@latest shortid@latest",
    "uni": "npm uninstall dotenv express mongoose body-parser cors shortid"
  },
  "author": "Jan Kozohorsk√Ω",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "env": {
      "es2021": true
    },
    "rules": {
      "require-await": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zigecek/api.git"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.0",
    "shortid": "^2.2.16"
  }
}
